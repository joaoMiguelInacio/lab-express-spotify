<header>
    <h1>Tracks in:  {{this.albumName}} </h1>
    <a href="/" class="homepage-button">Search for Something Different</a>
</header>


<div class = "tracks-row1">
        <p>Title</p>
        <p>Listen</p>
</div>
{{#each tracks}}
    <div class = "tracks-otherRows">
        <p>{{this.name}}</p>
        <audio controls src="{{this.preview_url}}"></audio>
    </div>
{{/each}}